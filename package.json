{
  "name": "golden-diff",
  "displayName": "Golden Diff",
  "publisher": "JeilsonAraujo",
  "description": "This help you to see easily differences between golden failures",
  "icon": "assets/golden_icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/jeilsonaraujo/golden_diff.git"
  },
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Testing"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "test": [
        {
          "id": "golden_failures",
          "name": "Golden Test Failures",
          "icon": "assets/golden_outlined.png",
          "contextualTitle": "Golden Test Failures"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "golden_failures.clear",
          "when": "view == golden_failures",
          "group": "navigation@0"
        },
        {
          "command": "golden_failures.refresh",
          "when": "view == golden_failures",
          "group": "navigation@0"
        }
      ]
    },
    "commands": [
      {
        "command": "golden_failures.item_clicked",
        "title": "tree view item"
      },
      {
        "command": "golden_failures.clear",
        "title": "Clear",
        "icon": {
          "light": "resources/light/clear.png",
          "dark": "resources/dark/clear.png"
        }
      },
      {
        "command": "golden_failures.refresh",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "image-size": "^1.0.2"
  }
}